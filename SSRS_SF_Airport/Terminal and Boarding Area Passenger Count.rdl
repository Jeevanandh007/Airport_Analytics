  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Shared_DataSource</DataSourceName>
        <CommandText>SELECT 
    t.Terminal,
    b.BoardingArea,
    SUM(f.PassengerCount) AS TotalPassengerCount
FROM 
    Fact_PassengerCount f
JOIN 
    Dim_Terminal t ON f.TerminalID = t.TerminalID
JOIN 
    Dim_BoardingArea b ON f.BoardingAreaID = b.BoardingAreaID
GROUP BY 
    t.Terminal, b.BoardingArea
ORDER BY 
    t.Terminal, b.BoardingArea;</CommandText>
      </Query>
      <Fields>
        <Field Name="Terminal">
          <DataField>Terminal</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="BoardingArea">
          <DataField>BoardingArea</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TotalPassengerCount">
          <DataField>TotalPassengerCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
